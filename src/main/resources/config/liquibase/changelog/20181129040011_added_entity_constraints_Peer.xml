<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity Peer.
    -->
    <changeSet id="20181129040011-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="score_id"
                                 baseTableName="peer"
                                 constraintName="fk_peer_score_id"
                                 referencedColumnNames="id"
                                 referencedTableName="score"/>

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="peer"
                                 constraintName="fk_peer_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

    </changeSet>
</databaseChangeLog>
