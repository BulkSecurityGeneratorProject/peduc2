<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity LectureActivity.
    -->
    <changeSet id="20181129040013-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="lecture_id"
                                 baseTableName="lecture_activity"
                                 constraintName="fk_lecture_activity_lecture_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lecture"/>

        <addForeignKeyConstraint baseColumnNames="lecture_activities_id"
                                 baseTableName="lecture_activity_user"
                                 constraintName="fk_lecture_activity_user_lecture_activities_id"
                                 referencedColumnNames="id"
                                 referencedTableName="lecture_activity"/>
        <addForeignKeyConstraint baseColumnNames="users_id"
                                 baseTableName="lecture_activity_user"
                                 constraintName="fk_lecture_activity_user_users_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>
        
    </changeSet>
</databaseChangeLog>
